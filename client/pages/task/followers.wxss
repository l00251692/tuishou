.container {
  padding-bottom: 0;
  background-color: #f0eff5;
  background-repeat: no-repeat;
  background-size: 100% auto;
  background-position: bottom center;
  background-image: url('data:image/png;base64,');
  min-height: 100%;
}

.datepick {
  width: 100%;
  position: relative;
  display: flex;
  margin: 0 4%;
  height: 70rpx;
  box-shadow: 5rpx 5rpx 15rpx #e2e2e2;
  transition: all 0.15s ease-in-out;
  margin-top:20rpx;
}

.datepick image{
  width:50rpx;
  height:50rpx;
  padding-left:30rpx;
  padding-right:10rpx;
  padding-top:10rpx;
}

.data_text {
  font-size: 12pt;
  line-height:70rpx;
  padding-left:20rpx;
  padding-right:30rpx;
  background: #fff;
}

.data {
  position: relative;
  font-size: 12pt;
  background: #fff;
  flex: 1;
  display: flex;
  height: 100%;
  padding-right: 20rpx;
  margin-right:30rpx;
  line-height:70rpx;
}

.picker {
  width: 94%;
  position: absolute;
  top: 100rpx;
  z-index: 200;
}

.book-list {
  display: flex;
  flex-direction: column;
  align-items: stretch;
  padding: 25rpx 30rpx;
  width: 100%;
  margin-left:30rpx;
  margin-right:30rpx;
}

.current-type {
  color: #777;
  font-size: 10pt;
  line-height: 200%;
  margin-bottom: 15rpx;
  margin-left: 30rpx;
}

.book-wraper {
  display: flex;
  align-items: center;
  justify-content: space-between;
  overflow: hidden;
  padding: 10rpx 0;
  margin-bottom: 10rpx;
  background: #fff;
  border-radius: 3px;
  flex-direction: row;
}

.book-name {
  font-size: 14pt;
  line-height: 80rpx;
  text-align: center;
  margin-left: 20rpx;
}

.book-count {
  font-size: 26pt;
  text-align: center;
  margin-right: 60rpx;
  color: #1e8cd4;
}

.book-info {
  flex: 1;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 10rpx 20rpx;
}

.book-date {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 20rpx;
  font-size: 11pt;
  color: #888;
}

.book-date text {
  line-height: 150%;
}

.book-img {
  height: 80rpx;
  width: 80rpx;
}

.note{
  width:100%;
  padding-left:30rpx;
  line-height: 60rpx;
  font-size: 12pt;
  margin-bottom:20rpx;
  border-bottom: 1px solid #fff;
}